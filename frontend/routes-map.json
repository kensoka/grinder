{ 
    "/api/bot/start": {
      "url": "/api/bot/start",
      "method": "POST",
      "description": "Запуск торгового бота",
      "to-server": {
        "тело-запроса": {
          "symbol": "BTCUSDT",
          "asset_a_funds": 700,
          "asset_b_funds": 0.01,
          "grids": 10,
          "deviation_threshold": 0.004,
          "growth_factor": 0.5,
          "use_granular_distribution": true,
          "trail_price": true,
          "only_profitable_trades": false
        }
      },
      "to-client-response": {
        "тело-ответа-в-случае-удачи": {
          "status": "Bot started successfully"
        },
        "тело-ответа-в-случае-неудачи": {
          "status": "error",
          "message": "Описание ошибки"
        }
      }
    },
  
    "/api/bot/stop": {
      "url": "/api/bot/stop",
      "method": "POST",
      "description": "Остановка торгового бота",
      "to-server": {},
      "to-client-response": {
        "тело-ответа-в-случае-удачи": {
          "status": "Bot stopped successfully"
        },
        "тело-ответа-в-случае-неудачи": {
          "status": "error",
          "message": "Описание ошибки"
        }
      }
    },
  
    "/ws": {
      "url": "/ws",
      "method": "WebSocket",
      "description": "WebSocket соединение для получения обновлений бота",
      "to-server": {
        "сообщения": {
          "subscribe": {
            "type": "subscribe",
            "symbol": "BTCUSDT",
            "interval": "1m"
